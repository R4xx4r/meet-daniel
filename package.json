{
  "name": "meet-daniel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "icons": "vsvg -s src/assets/svg -t src/components/icons",
    "serve": "npm run icons && vue-cli-service serve",
    "build": "npm run icons && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.4.4",
